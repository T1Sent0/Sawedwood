        div.section.sixthPage(data-anchor='sixthPage')#sixthPage
            div.container-page
                h1 Как мы работаем
                div.container-etap
                   include ../../components/etaps/etapt
                   +createEtap(require('./img/how-we-worked/1.svg'), '1.', 'Заказ')
                   +createEtap(require('./img/how-we-worked/2.svg'), '2.', 'Согласование дизайна')
