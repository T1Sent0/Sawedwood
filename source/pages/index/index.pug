                div.full-information-material.swiper-container
                    div.swiper-wrapper
                        mixin createSlideMaterial (imgPaht, title, prev, next, textFirstP, textSecondP, textThreedP)
                            div.swiper-slide
                                div.inform
                                    img.igm(src=imgPaht)
                                    div.close.close-slider
                                    span.title=title
                                    p=textFirstP
                                    p=textSecondP
                                    p.last=textThreedP
